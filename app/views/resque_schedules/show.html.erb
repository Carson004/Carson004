  <div class="row">
    <div class="col-xs-4 col-sm-2">
      <%= ResqueSchedule.human_attribute_name :class_name %>
    </div>
    <div class="col-xs-8 col-sm-10">
      <%= @resque_schedule.class_name %>
    </div>
  </div>
  <div class="row">
    <div class="col-xs-4 col-sm-2">
      <%= ResqueSchedule.human_attribute_name :queue %>
    </div>
    <div class="col-xs-8 col-sm-10">
      <%= @resque_schedule.queue %>
    </div>
  </div>
  <div class="row">
    <div class="col-xs-4 col-sm-2">
      <%= ResqueSchedule.human_attribute_name :description %>
    </div>
    <div class="col-xs-8 col-sm-10">
      <%= @resque_schedule.description %>
    </div>
  </div>
  <div class="row">
    <div class="col-xs-4 col-sm-2">
      <%= ResqueSchedule.human_attribute_name :enabled %>
    </div>
    <div class="col-xs-8 col-sm-10">
      <%= @resque_schedule.enabled %>
    </div>
  </div>
</div>
